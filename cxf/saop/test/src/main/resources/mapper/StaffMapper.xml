<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.siniswift.mapper.StaffMapper" >

    <insert id="save" parameterType="com.siniswift.entity.StaffEntity" >
   	 INSERT INTO "TEST"."STAFF" ("id", "name", "state")
     VALUES (#{id,jdbcType=NUMERIC}, #{name,jdbcType=VARCHAR}, #{state,jdbcType=NUMERIC})
    </insert>

</mapper>